[gui]
	recentrepo = C:/Users/user/rep
[user]
	name = Diyar
	email = traunmeerbekov@gmail.com
[init]
	defaultBranch = main
[core]
	autocrlf = true
	safecrlf = warn
	editor = code --wait
[diff]
    tool = default-difftool
[difftool "default-difftool"]
    cmd = code --wait --diff $LOCAL $REMOTE
[merge]
    tool = code
[mergetool "code"]
    cmd = code --wait --merge $REMOTE $LOCAL $BASE $MERGED
